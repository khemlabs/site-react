{
	"name": "next",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"dev": "nodemon",
		"build": "next build && cp -r ./public ./dist/ && tsc --project tsconfig.server.json",
		"start": "cross-env NODE_ENV=production node dist/index.js",
		"deploy": "yarn build && yarn start",
		"type-check": "tsc --project tsconfig.server.json",
		"docker-dev-build": "docker-compose build --no-cache --force-rm && yarn docker-install",
		"docker-dev-down": "docker-compose down -v --remove-orphans",
		"docker-dev": "docker-compose up -d",
		"docker-start": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d",
		"docker-install": "docker-compose run app yarn install",
		"docker-logs": "docker-compose logs -f"
	},
	"dependencies": {
		"@material-ui/core": "^4.7.1",
		"@material-ui/icons": "^4.5.1",
		"axios": "^0.21.4",
		"cross-env": "^6.0.3",
		"express": "^4.17.1",
		"formik": "^2.1.2",
		"formik-material-ui": "^2.0.0-alpha.3",
		"next": "^9.1.4",
		"next-i18next": "^2.1.1",
		"nodemailer": "^6.6.5",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-recaptcha": "^2.3.10",
		"react-spring": "^8.0.27",
		"redis": "^2.8.0"
	},
	"devDependencies": {
		"@types/express": "^4.17.2",
		"@types/google.analytics": "^0.0.40",
		"@types/gtag.js": "^0.0.3",
		"@types/next": "^9.0.0",
		"@types/node": "^12.12.14",
		"@types/nodemailer": "^6.4.0",
		"@types/react": "^16.9.15",
		"@types/react-recaptcha": "^2.3.3",
		"@types/redis": "^2.8.14",
		"nodemon": "^2.0.2",
		"ts-node": "^8.5.4",
		"typescript": "^3.7.3"
	}
}
