		"redis": "^2.8.0"
	},
	"devDependencies": {
		"@types/express": "^4.17.2",
		"@types/next": "^9.0.0",
		"@types/node": "^12.12.14",
		"@types/nodemailer": "^6.4.0",
		"@types/react": "^16.9.15",
